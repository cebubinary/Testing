@model IEnumerable<WebApplication4.Models.Employee>

@Html.ActionLink("Create", "Edit")
<br />
<text>**********************************</text>
<br />
@foreach(var item in Model)
{
  
    @Html.LabelFor(a => item.Name) <text>: &nbsp;</text>
    @Html.DisplayFor(a => item.Name)

    <br />
    @Html.LabelFor(a => item.Address) <text>: &nbsp;</text>
    @Html.DisplayFor(a => item.Address)
